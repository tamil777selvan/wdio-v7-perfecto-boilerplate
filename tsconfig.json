{
    "compilerOptions": {
        "outDir": "./dist",
        "sourceMap": false,
        "target": "es2019",
        "module": "commonjs",
        "removeComments": true,
        "noImplicitAny": true,
        "strictPropertyInitialization": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "types": [
            "node",
            "webdriverio/async",
            "@wdio/cucumber-framework"
        ],
    },
    "include": [
        "./src/**/*.ts"
    ]
}
